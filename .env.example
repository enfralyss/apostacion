# Copy this file to .env and fill in your credentials

# TriunfoBet Credentials
TRIUNFOBET_USER=your_username_here
TRIUNFOBET_PASS=your_password_here

# Telegram Bot (for notifications)
TELEGRAM_BOT_TOKEN=your_bot_token_here
# Pool de usuarios para notificaciones (separados por coma)
# Formato: chat_id1,chat_id2,chat_id3
TELEGRAM_CHAT_IDS=123456789,987654321

# The Odds API - Pool de keys (separadas por coma para rotaci√≥n)
# Cada key = 500 req/mes. Con 3 keys = 1500 req/mes
ODDS_API_KEYS=key1_here,key2_here,key3_here

# API Keys adicionales
FOOTBALL_DATA_API_KEY=your_api_key_here
NBA_STATS_API_KEY=your_api_key_here

# Database (if using PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=triunfobet_db
DB_USER=postgres
DB_PASS=your_password_here
